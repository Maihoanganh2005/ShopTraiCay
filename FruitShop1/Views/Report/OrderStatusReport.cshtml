@model IEnumerable<dynamic>

<h2>Thống kê đơn hàng theo trạng thái</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Trạng thái đơn hàng</th>
            <th>Số lượng</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Status</td>
                <td>@item.Count</td>
            </tr>
        }
    </tbody>
</table>
